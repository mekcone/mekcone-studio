{
    "name": "spring-boot",
    "version": "1.0.0",
    "metadata": {
        "javaVersion": "8",
        "springBootVersion": "2.7"
    },
    "structure": {
        "name": "spring-boot",
        "type": "dir",
        "children": [
            {
                "name": "pom.xml",
                "type": "file",
                "dynamic": true,
                "source": "content/pom/default.xml",
                "transformer": "process_pom"
            },
            {
                "name": "src",
                "type": "dir",
                "children": [
                    {
                        "name": "main",
                        "type": "dir",
                        "children": [
                            {
                                "name": "java",
                                "type": "dir",
                                "children": [
                                    {
                                        "name": "${template.properties.project.groupId}/${template.properties.project.artifactId}",
                                        "type": "dir",
                                        "children": [
                                            {
                                                "name": "Application.java",
                                                "type": "file",
                                                "dynamic": true,
                                                "source": "content/Application.java",
                                                "transformer": "greet"
                                            },
                                            {
                                                "name": "config",
                                                "type": "dir"
                                            },
                                            {
                                                "name": "controller",
                                                "type": "dir"
                                            },
                                            {
                                                "name": "entity",
                                                "type": "dir"
                                            },
                                            {
                                                "name": "service",
                                                "type": "dir"
                                            },
                                            {
                                                "name": "util",
                                                "type": "dir"
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "name": "resources",
                                "type": "dir",
                                "children": [
                                    {
                                        "name": "application.properties",
                                        "type": "file",
                                        "dynamic": true,
                                        "source": "content/properties/default.properties"
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "name": "test",
                        "type": "dir",
                        "children": [
                            {
                                "name": "java",
                                "type": "dir"
                            }
                        ]
                    }
                ]
            }
        ]
    }
}