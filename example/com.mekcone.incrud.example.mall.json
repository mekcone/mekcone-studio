{
    "groupId": "com.mekcone.incrud.example",
    "artifactId": "mall",
    "version": "0.0.1",
    "description": "MekCone Mall Project",
    "dependencies": [
        {
            "label": "mysql",
            "groupId": "mysql",
            "artifactId": "mysql-connector-java",
            "version": "8.0.19",
            "properties": [
                {
                    "key": "spring.datasource.url",
                    "value": "jdbc:mysql://gakiton.cn:3306/mall"
                },
                {
                    "key": "spring.datasource.username",
                    "value": "root"
                },
                {
                    "key": "spring.datasource.password",
                    "value": "xxxxxx"
                },
                {
                    "key": "spring.datasource.driver-class-name",
                    "value": "com.mysql.cj.jdbc.Driver"
                }
            ]
        },
        {
            "label": "spring-boot-web",
            "groupId": "org.springframework.boot",
            "artifactId": "spring-boot-starter-web"
        },
        {
            "label": "mybatis",
            "groupId": "org.mybatis.spring.boot",
            "artifactId": "mybatis-spring-boot-starter",
            "version": "2.1.1"
        }
    ],
    "tables": [
        {
            "name": "goods",
            "columns": [
                {
                    "name": "goods_id",
                    "type": "int",
                    "primaryKey": true
                },
                {
                    "name": "name",
                    "type": "varchar(50)"
                },
                {
                    "name": "description",
                    "type": "varchar(50)"
                }
            ]
        },
        {
            "name": "cart",
            "columns": [
                {
                    "name": "cart_id",
                    "type": "int",
                    "primaryKey": true
                },
                {
                    "name": "goods_id",
                    "type": "varchar(50)"
                },
                {
                    "name": "user_id",
                    "type": "varchar(50)"
                }
            ]
        }
    ]
}